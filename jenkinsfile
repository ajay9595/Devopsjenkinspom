pipeline {
    agent any
    tools
    {
    maven
    }

    stages {
        stage('compiole') {
            steps {
                echo 'compiling'
                sh 'mvn compile'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                sh 'mvn test'
            }
        }
        stage('package') {
            steps {
                echo 'packaging....'
                sh 'mvn package'
            }
        }
    }
}
